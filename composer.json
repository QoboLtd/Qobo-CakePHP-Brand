{
    "name": "Qobo/Qobo-CakePHP-Brand",
    "description": "Plugin to add brand to website footer",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Qobo\\QoboCakePHPBrand\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qobo\\QoboCakePHPBrand\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
